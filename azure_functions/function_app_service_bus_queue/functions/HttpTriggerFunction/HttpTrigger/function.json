{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "serviceBus",
      "direction": "out",
      "name": "res",
      "queueName": "service-bus-queue-queue",
      "connection": "AzureWebJobsServiceBus"
    }
  ],
  "scriptFile": "../dist/HttpTrigger/index.js"
}